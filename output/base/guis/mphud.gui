    //
    //  Clip Low / Clip Empty Changes
    //
	onNamedEvent updateAmmo {
		if ("weaphud" == 0) {
			if ("gui::player_clip_low" == 1) {
				set "Lines2A::matcolor" "1 0.5 0.2 0.2" ;
				set "ammovalue2::forecolor" "1 0.6 0 1" ;
				set "BorderLA::visible" "0" ;
				set "BorderLA_1::visible" "1" ;
				set "BorderLA_2::visible" "0" ;
			} else {
				set "Lines2A::matcolor" "0.3 0.8 0.7 0.2" ;
				set "ammovalue2::forecolor" "0.67 0.7 0.7 1" ;
				set "BorderLA::visible" "1" ;
				set "BorderLA_1::visible" "0" ;
				set "BorderLA_2::visible" "0" ;
			}

			if ("gui::player_clip_empty" == 1) {
				set "Lines2A::matcolor" "1 0 0 0.2" ;
				set "ammovalue2::forecolor" "1 0 0 1" ;
				set "BorderLA::visible" "0" ;
				set "BorderLA_1::visible" "0" ;
				set "BorderLA_2::visible" "1" ;
			}

			if ("gui::player_ammo_empty" == 1) {
				set "Lines1A::matcolor" "1 0 0 0.2" ;
				set "Lines2A::matcolor" "1 0 0 0.2" ;
				set "ammovalue2::forecolor" "1 0 0 1" ;
				set "ammovalue3::forecolor" "1 0 0 1" ;
				set "BorderLA::visible" "0" ;
				set "BorderLA_1::visible" "1" ;
			} else {
				set "Lines1A::matcolor" "0.3 0.8 0.7 0.2" ;
				set "ammovalue3::forecolor" "0.35 0.42 0.4 1" ;
			}
		}
	}

