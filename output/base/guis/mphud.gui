	onNamedEvent updateArmorHealthAir {
		if ("gui::player_armor"  > 0) {
			set 	"Lines1::matcolor" "0.3 0.8 0.7 0.2" ;
			set	"MPArmor::forecolor" "0.35 0.42 0.4 1" ;
			set 	"Lines1::visible" "1" ;
